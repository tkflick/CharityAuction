<?xml version="1.0"?>
<configuration>
  <appSettings>
    <add key="APP_NAME" value="CharityAuction"/>
    <add key="APP_VERSION" value="1.02"/>
    <add key="oCNX" value="Provider=Microsoft.ACE.OLEDB.12.0;Data Source=XXREPLACEXX;Persist Security Info=False;"/>
    <add key="user_db" value="~/App_Data/tUserData.accdb"/>
    <add key="auction_db" value="~/App_Data/AuctionTrax.accdb"/>
    <add key="EncryptionKey" value="5"/>
    <add key="ValidationSettings:UnobtrusiveValidationMode" value="None" />
    <!-- DO NOT CHANGE ANYTHING ABOVE THIS LINE -->
    <!-- CONFIGURE THE SITE TO SHOW YOUR CLUB'S BRANDING BELOW -->
    <!-- Start Date/Time. Set in the past, the auction is immediately active.  Set in the future to start on a given day -->
    <add key="LockBidDate" value="22-AUG-2020 09:00"/>
    <!-- Bid increment -->
    <add key="BidIncrement" value="1.00"/>
    <!-- comma delimited list of user names that will have access to the admin tools -->    
    <add key="AdminUser" value="tkflick" />
    <!-- FACEBOOK URL. If your club doesn't have one, use Lions International https://www.facebook.com/lionsclubs/ -->
