    <!-- Club website URL -->
    <add key="WebURL" value="http://www.windcrestlionsclub.org" />
    <!-- clubs informal name as you want it to appear on the site -->
    <add key="CLUB_NAME" value="Windcrest Lions Club" />
    <!-- full name of your club, example: the Windcrest Lions Club's actual registered name is San Antonio Windcrest Lions Club -->
    <add key="FULL_CLUB_NAME" value="San Antonio Windcrest Lions Club" />
    <!-- this is the district in which your club is located. example: 2-A2 -->
    <add key="DISTRICT" value="2-A2" />
    <!-- location, ie City, State - Country -->
    <add key="LOCATION" value="Windcrest, Texas - United States" />
    <!-- email address you want to receive the emails from the site, such as winning bidders and paypal notifications -->
    <add key="CLUB_EMAIL" value="windcrestlions.smile@gmail.com" />
    <!-- webmaster email, also used in the mailSettings below -->
    <add key="WEBMASTER_EMAIL" value="webmaster@windcrestlionsclub.org" />
